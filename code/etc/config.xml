<config>
    <modules>
        <Mbiz_Logout>
            <version>1.0.0</version>
        </Mbiz_Logout>
    </modules>
    <global>
        <events>
            <controller_action_postdispatch_customer_account_logout>
                <observers>
                    <mbiz_logout>
                        <type>singleton</type>
                        <class>Mbiz_Logout_Model_Observer</class>
                        <method>removeRedirection</method>
                    </mbiz_logout>
                </observers>
            </controller_action_postdispatch_customer_account_logout>
        </events>
    </global>
</config>
